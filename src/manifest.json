{
  "name": "cuckoo",
  "version": "0.3.0",
  "description": "Free, powerful tool for image cloud",
  "icons": {
    "128": "style/images/icon_128.png"
  },
  "homepage_url": "http://codante.org",
  "background": {
    "scripts": [
      "js/background/chrome.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "19": "style/images/icon_19.png",
      "38": "style/images/icon_38.png"
    }
  },
  "manifest_version": 2
}